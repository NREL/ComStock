  <class_name>UpgradeAddThermostatSetback</class_name>
  <display_name>Upgrade_Add_Thermostat_Setback</display_name>
  <description>This measure implements thermostat setbacks during unoccupied periods.</description>
  <modeler_description>This measure implements thermostat setbacks during unoccupied periods.</modeler_description>
  <arguments>
    <argument>
      <name>clg_setback</name>
      <display_name>Cooling setback magnitude</display_name>
      <description>Setback magnitude in cooling.</description>
      <type>Integer</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>5</default_value>
    </argument>
    <argument>
      <name>htg_setback</name>
      <display_name>Heating setback magnitude</display_name>
      <description>Setback magnitude in heating.</description>
      <type>Integer</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>5</default_value>
    </argument>
    <argument>
      <name>opt_start</name>
      <display_name>Model an optimum start different from what currently exists?</display_name>
      <description>True if yes; false if no. If false, any existing optimum starts will be preserved.</description>
      <type>Boolean</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>true</default_value>
      <choices>
        <choice>
          <value>true</value>
          <display_name>true</display_name>
        </choice>
        <choice>
          <value>false</value>
          <display_name>false</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>opt_start_len</name>
      <display_name>Length of optimum start.</display_name>
      <description>Length of period (in hours) over which optimum start takes place before occupancy. If previous argument is false, this option is disregarded.</description>
      <type>Integer</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>3</default_value>
    </argument>
    <argument>
      <name>htg_min</name>
      <display_name>Minimum heating setpoint</display_name>
      <description>Minimum heating setpoint</description>
      <type>Integer</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>60</default_value>
    </argument>
    <argument>
      <name>clg_max</name>
      <display_name>Maximum cooling setpoint</display_name>
      <description>Maximum cooling setpoint</description>
      <type>Integer</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>82</default_value>
    </argument>
  </arguments>
