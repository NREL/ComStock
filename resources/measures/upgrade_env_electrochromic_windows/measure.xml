  <class_name>ElectrochromicWindowsModulating</class_name>
  <display_name>Electrochromic Windows Modulating</display_name>
  <description>Adds electrochromic windows. SHGC and VLT will modulate linearly between specified clear and tinted properties.</description>
  <modeler_description>Adds electrochromic windows. SHGC and VLT will modulate linearly between specified clear and tinted properties.</modeler_description>
  <arguments>
    <argument>
      <name>dynamic_sgs_upgrade</name>
      <display_name>Dynamic SGS Upgrade</display_name>
      <description>Identify electrochromic secondary glazing technology to be applied to entire building.</description>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>high_perf_low_tech</default_value>
      <choices>
        <choice>
          <value>high_perf_low_tech</value>
          <display_name>high_perf_low_tech</display_name>
        </choice>
        <choice>
          <value>high_perf_high_tech</value>
          <display_name>high_perf_high_tech</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>u_value</name>
      <display_name>U-value (Btu/h·ft2·°F)</display_name>
      <description>Replaces u-value of existing applicable windows with this value. Use IP units.</description>
      <type>Double</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>0.322</default_value>
    </argument>
    <argument>
      <name>max_rad_w_per_m2</name>
      <display_name>Maximum Allowable Radiation (W/m^2)</display_name>
      <description>Windows will switch to a darker state if threshold is exceeded.</description>
      <type>Double</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>180</default_value>
    </argument>
    <argument>
      <name>gi</name>
      <display_name>Maximum Allowable Glare Index</display_name>
      <description>Sets maximum glare index allowance for EC windows.</description>
      <type>Double</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>24</default_value>
    </argument>
    <argument>
      <name>min_temp</name>
      <display_name>Minimum Temperature for EC Tinting (F)</display_name>
      <description>Sets minimum temperature to allow for EC tinting. A low value will mitigate heating penalties.</description>
      <type>Double</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>55</default_value>
    </argument>
    <argument>
      <name>ec_priority_logic</name>
      <display_name>Prioritize Glare Or Temperature?</display_name>
      <description>Select whether to prioritize glare or outdoor temperature when deciding EC state.</description>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>glare</default_value>
      <choices>
        <choice>
          <value>glare</value>
          <display_name>glare</display_name>
        </choice>
        <choice>
          <value>temperature</value>
          <display_name>temperature</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>North</name>
      <display_name>Apply electrochromic glazing to North facade windows.</display_name>
      <type>Boolean</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>true</default_value>
      <choices>
        <choice>
          <value>true</value>
          <display_name>true</display_name>
        </choice>
        <choice>
          <value>false</value>
          <display_name>false</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>East</name>
      <display_name>Apply electrochromic glazing to East facade windows.</display_name>
      <type>Boolean</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>true</default_value>
      <choices>
        <choice>
          <value>true</value>
          <display_name>true</display_name>
        </choice>
        <choice>
          <value>false</value>
          <display_name>false</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>South</name>
      <display_name>Apply electrochromic glazing to South facade windows.</display_name>
      <type>Boolean</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>true</default_value>
      <choices>
        <choice>
          <value>true</value>
          <display_name>true</display_name>
        </choice>
        <choice>
          <value>false</value>
          <display_name>false</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>West</name>
      <display_name>Apply electrochromic glazing to West facade windows.</display_name>
      <type>Boolean</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>true</default_value>
      <choices>
        <choice>
          <value>true</value>
          <display_name>true</display_name>
        </choice>
        <choice>
          <value>false</value>
          <display_name>false</display_name>
        </choice>
      </choices>
    </argument>
  </arguments>
