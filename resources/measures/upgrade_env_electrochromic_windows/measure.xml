  <class_name>ElectrochromicWindowsModulating</class_name>
  <display_name>Electrochromic Windows Modulating</display_name>
  <description>Adds electrochromic windows. SHGC and VLT will modulate linearly between specified clear and tinted properties.</description>
  <modeler_description>Adds electrochromic windows. SHGC and VLT will modulate linearly between specified clear and tinted properties.</modeler_description>
  <arguments>
    <argument>
      <name>shgc_clear</name>
      <display_name>SHGC Clear</display_name>
      <description>Sets SHGC for clear state. Value of 0 will use current glazing property.</description>
      <type>Double</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>0.319</default_value>
    </argument>
    <argument>
      <name>vt_clear</name>
      <display_name>VT Clear</display_name>
      <description>Sets VT for clear state. Value of 0 will use current glazing property.</description>
      <type>Double</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>0.396</default_value>
    </argument>
    <argument>
      <name>shgc_tinted</name>
      <display_name>SHGC Tinted</display_name>
      <description>Sets SHGC for tinted state.</description>
      <type>Double</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>0.077</default_value>
    </argument>
    <argument>
      <name>vt_tinted</name>
      <display_name>VT Tinted</display_name>
      <description>Sets VT for tinted state.</description>
      <type>Double</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>0.007</default_value>
    </argument>
    <argument>
      <name>u_value</name>
      <display_name>U-value (Btu/h·ft2·°F)</display_name>
      <description>Replaces u-value of existing applicable windows with this value. Use IP units.</description>
      <type>Double</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>0.322</default_value>
    </argument>
    <argument>
      <name>max_rad_w_per_m2</name>
      <display_name>Maximum Allowable Radiation (W/m^2)</display_name>
      <description>Windows will switch to a darker state if threshold is exceeded.</description>
      <type>Double</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>180</default_value>
    </argument>
    <argument>
      <name>gi</name>
      <display_name>Maximum Allowable Glare Index</display_name>
      <description>Sets maximum glare index allowance for EC windows.</description>
      <type>Double</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>24</default_value>
    </argument>
    <argument>
      <name>min_temp</name>
      <display_name>Minimum Temperature for EC Tinting (F)</display_name>
      <description>Sets minimum temperature to allow for EC tinting. A low value will mitigate heating penalties.</description>
      <type>Double</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>55</default_value>
    </argument>
    <argument>
      <name>ec_priority_logic</name>
      <display_name>Prioritize Glare Over Temperature?</display_name>
      <type>Boolean</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>true</default_value>
      <choices>
        <choice>
          <value>true</value>
          <display_name>true</display_name>
        </choice>
        <choice>
          <value>false</value>
          <display_name>false</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
