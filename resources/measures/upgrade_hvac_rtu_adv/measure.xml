        </choice>
      </choices>
    </argument>
    <argument>
      <name>roof</name>
      <display_name>Upgrade Roof Insulation?</display_name>
      <description>Upgrade roof insulation per AEDG recommendations.</description>
      <type>Boolean</type>
      <required>true</required>
