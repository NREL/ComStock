  <class_name>SetServiceWaterHeatingFuel</class_name>
  <display_name>set_service_water_heating_fuel</display_name>
  <description>Changes natural gas water heaters to either fuel oil or propane, and changes electric water heaters to district heating. Not applicable when the input service_water_heating_fuel is Electricity or Natural Gas, as the fuels for those inputs are already set when the water heaters are added to the model.</description>
  <modeler_description></modeler_description>
  <arguments>
    <argument>
      <name>service_water_heating_fuel</name>
      <display_name>Service Water Heating Fuel</display_name>
      <description>The primary fuel used for service water heating in the model.</description>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>NaturalGas</default_value>
      <choices>
        <choice>
          <value>NaturalGas</value>
          <display_name>NaturalGas</display_name>
        </choice>
        <choice>
          <value>Electricity</value>
          <display_name>Electricity</display_name>
        </choice>
        <choice>
          <value>FuelOil</value>
          <display_name>FuelOil</display_name>
        </choice>
        <choice>
          <value>Propane</value>
          <display_name>Propane</display_name>
        </choice>
        <choice>
          <value>DistrictHeating</value>
          <display_name>DistrictHeating</display_name>
        </choice>
      </choices>
    </argument>
  </arguments>
