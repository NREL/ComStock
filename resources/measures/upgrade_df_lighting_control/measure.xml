  <class_name>DFLightingControl</class_name>
  <display_name>demand flexibility - lighting control</display_name>
  <description>This measure implements demand flexibility measure on daily lighting control with load shed strategy, by adjusting Lighting Power Density (reflecting lighting dimming) corresponding to the peak schedule based on daily peak load prediction.</description>
  <modeler_description>This measure</modeler_description>
  <arguments>
    <argument>
      <name>demand_flexibility_objective</name>
      <display_name>Objective of demand flexibility control (peak load, emission, utility bill cost, operational cost)</display_name>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>peak load</default_value>
      <choices>
        <choice>
          <value>peak load</value>
          <display_name>peak load</display_name>
        </choice>
        <choice>
          <value>emission</value>
          <display_name>emission</display_name>
        </choice>
        <choice>
          <value>utility bill cost</value>
          <display_name>utility bill cost</display_name>
        </choice>
        <choice>
          <value>operational cost</value>
          <display_name>operational cost</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>peak_len</name>
      <display_name>Length of dispatch window (hour)</display_name>
      <type>Integer</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>4</default_value>
    </argument>
    <argument>
      <name>light_adjustment_method</name>
      <display_name>Method of lighting dimming (absolute change, relative change)</display_name>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>absolute change</default_value>
      <choices>
        <choice>
          <value>absolute change</value>
          <display_name>absolute change</display_name>
        </choice>
        <choice>
          <value>relative change</value>
          <display_name>relative change</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>light_adjustment</name>
      <display_name>Percentage to decrease light dimming/LPD by (0-100)</display_name>
      <type>Double</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>30</default_value>
    </argument>
    <argument>
      <name>num_timesteps_in_hr</name>
      <display_name>Number/Count of timesteps in an hour for sample simulations</display_name>
      <type>Integer</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>4</default_value>
    </argument>
    <argument>
      <name>load_prediction_method</name>
      <display_name>Method of load prediction or window determination reference</display_name>
      <description>Load prediction based on full baseline run, load prediction based on bin sample simulation, load prediction base on part year bin sample simulation, predetermined fixed schedule (no prediction), load peak prediction based on outdoor air temperature)</description>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>full baseline</default_value>
      <choices>
        <choice>
          <value>full baseline</value>
          <display_name>full baseline</display_name>
        </choice>
        <choice>
          <value>bin sample</value>
          <display_name>bin sample</display_name>
        </choice>
        <choice>
          <value>part year bin sample</value>
          <display_name>part year bin sample</display_name>
        </choice>
        <choice>
          <value>fix</value>
          <display_name>fix</display_name>
        </choice>
        <choice>
          <value>oat</value>
          <display_name>oat</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>peak_lag</name>
      <display_name>Time lag of peak responding to temperature peak (hour), for oat prediction method only</display_name>
      <type>Integer</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>2</default_value>
    </argument>
    <argument>
      <name>peak_window_strategy</name>
      <display_name>Method of determining peak windows (max savings, start with peak, end with peak, center with peak)</display_name>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>center with peak</default_value>
      <choices>
        <choice>
          <value>max savings</value>
          <display_name>max savings</display_name>
        </choice>
        <choice>
          <value>start with peak</value>
          <display_name>start with peak</display_name>
        </choice>
        <choice>
          <value>end with peak</value>
          <display_name>end with peak</display_name>
        </choice>
        <choice>
          <value>center with peak</value>
          <display_name>center with peak</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
