<measure>
  <schema_version>3.1</schema_version>
  <name>add_heat_pump_rtu</name>
  <uid>f4567a68-27f2-4a15-ae91-ba0f35cd08c7</uid>
  <version_id>bc03ee8d-b985-41c3-896a-91fa3e898988</version_id>
